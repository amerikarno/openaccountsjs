<script>
    const MAIN_URL = window.location.origin;

    $(document).ready(function () {
        let v = parseInt('1');
        $('#waiting_modal_dialog').addClass('show');
        if (v == 1) {
            setTimeout(() => {
                $('#waiting_modal_dialog').removeClass('show');
            }, 300);
        } else {
            setTimeout(() => {
                window.location.href = '/send-reload-page';
            }, 1000);
        }

        function disableBack() {
            window.history.forward()
        }
        window.onload = disableBack();
        window.onpageshow = function (e) {
            if (e.persisted)
                disableBack();
        }
    })

    function selectDoc(type) {
        if (type == '3') {
            data =
                '<input type="text"  id="Idcard" name="Idcard" data-validation="required" data-validation-error-msg="กรุณาระบุหมายเลข passport"/>' +
                '<span class="highlight"></span>' +
                '<span class="bar"></span>' +
                '<label for="Idcard">หมายเลข passport  <span>*</span></label>'
        } else if (type == '2') {
            data =
                '<input type="text"  id="Idcard" name="Idcard" data-validation="required" data-validation-error-msg="กรุณาระบุหมายเลขต่างด้าว"/>' +
                '<span class="highlight"></span>' +
                '<span class="bar"></span>' +
                '<label for="Idcard">หมายเลขต่างด้าว  <span>*</span></label>'
        } else {
            data =
                '<input type="text" id="Idcard" name="Idcard" data-validation-error-msg="กรุณาระบุหมายเลขบัตรประจำตัวประชาชน" data-validation="idcard" data-validation-length="min13" maxlength="13" onkeyup="checkForm();"/>' +
                '<span class="highlight"></span>' +
                '<span class="bar"></span>' +
                '<span class="right"><img src="/assets/imgs/check.svg" alt=""></span>' +
                '<label for="Idcard">หมายเลขบัตรประจำตัวประชาชน  <span>*</span></label>'
        }
        $("#Card").html(data)
    }

    function checkID(id) {
        if (id.length != 13)
            return false;
        for (i = 0, sum = 0; i < 12; i++) {
            sum += parseFloat(id.charAt(i)) * (13 - i);
        }
        if ((11 - sum % 11) % 10 != parseFloat(id.charAt(12))) {
            return false;
        }
        return true;
    }

    function btnCounterService() {
        const wsurl = `${MAIN_URL}/api/clear-view-count`;
        var data = {
            url: ""
        };
        fetch(wsurl, {
            method: "POST",
            body: JSON.stringify(data)
        })
            .then(response => response.text())
            .then(function (result) {
                window.open(`${MAIN_URL}/counterservice`, "_self");
            })
            .catch(error => console.log('error', error));
    }

    function btnNdid() {
        const wsurl = `${MAIN_URL}/api/clear-view-count`;
        var data = {
            url: ""
        };
        fetch(wsurl, {
            method: "POST",
            body: JSON.stringify(data)
        })
            .then(response => response.text())
            .then(function (result) {
                window.open(`${MAIN_URL}/NDID-termandcondition`, "_self");
            })
            .catch(error => console.log('error', error));
    }

    function checkForm() {
        if (!checkID($("#Idcard").val()))
            $('.right').fadeOut()
        else
            $('.right').fadeIn()
    }
</script>

</body>

</html>
